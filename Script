local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("DrambobCool", "DarkTheme")
_G.autoTrueKill = true
_G.autoTrueBuff = true
_G.autoTrueAuto = true

--Main
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")

MainSection:NewToggle("EZ-Train", "Gives you a large amount of Clicks.", function(state)
    if state then
    _G.autoTrueKill = true
        while _G.autoTrueKill == true do
-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "13"
}

game:GetService("ReplicatedStorage").Signals.RemoteEvents.SellRemote:FireServer(unpack(args))

wait(0.00000001)
end
    else
        _G.autoTrueKill = false
    end
end)

MainSection:NewToggle("EZ-Train", "Gives you a large amount of Clicks.", function(state)
    if state then
    _G.autoTrueKill = true
        while _G.autoTrueKill == true do

wait(0.01)
end
    else
        _G.autoTrueKill = false
    end
end)
